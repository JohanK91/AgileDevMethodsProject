<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="code.Controller.driverDrivingController">
   <children>
      <Label fx:id="taskAddressText" layoutX="115.0" layoutY="93.0" text="ERROR" />
      <Label layoutX="60.0" layoutY="93.0" text="Task is at: " />
      <Button fx:id="arrivedButton" alignment="CENTER" layoutX="126.0" layoutY="277.0" mnemonicParsing="false" onAction="#arrivedPressed" text="Arrived" />
      <Button fx:id="taskPickedUpButton" alignment="CENTER" layoutX="126.0" layoutY="277.0" mnemonicParsing="false" onAction="#taskPickedUpPressed" text="Picked Up Items" visible="false" />
      <Button fx:id="deliveredTasksButton" alignment="CENTER" layoutX="126.0" layoutY="277.0" mnemonicParsing="false" onAction="#deliveredTasksPressed" text="Delivered Items" visible="false" />
      <Button fx:id="continueDriveButton" alignment="CENTER" layoutX="128.0" layoutY="277.0" mnemonicParsing="false" onAction="#continueDrivePressed" text="Continue drive" textAlignment="CENTER" visible="false" />
      <Button fx:id="taskIncompleteButton" layoutX="242.0" layoutY="277.0" mnemonicParsing="false" onAction="#taskIncompletePressed" text="Unable to pick up items" visible="false" />
   </children>
</AnchorPane>
